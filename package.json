{
  "name": "consultant-chat",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx server.ts",
    "build": "npm run build:client && npm run build:server && npm run copy:function",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "copy:function": "mkdir -p netlify/functions && cp -r dist/server/* netlify/functions/ && cp netlify/functions/render.ts netlify/functions/",
    "serve": "NODE_ENV=production tsx server.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "migrate-sparks": "NODE_ENV=development tsx scripts/migrate-sparks.ts",
    "inject-demo-consultant": "NODE_ENV=development tsx scripts/injectDemoConsultant.ts",
    "generate-brand-assets": "NODE_ENV=development tsx scripts/generateBrandAssets.ts"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@supabase/supabase-js": "^2.47.13",
    "@types/matter-js": "^0.19.8",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-helmet-async": "^1.0.1",
    "framer-motion": "^11.17.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.344.0",
    "matter-js": "^0.20.0",
    "openai": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@netlify/functions": "^3.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.0",
    "compression": "^1.8.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "postcss": "^8.5.2",
    "puppeteer": "^24.2.1",
    "sirv": "^3.0.1",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^3.12.10",
    "typescript": "^5.2.0",
    "vite": "^5.1.4"
  }
}
